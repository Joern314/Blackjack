{

    "manifest_version": 2,
    "name": "Blackjack",
    "version": "1.0",

    "description": "Various Addons for the QED-Chat.",

    "icons": {
        "48": "icons/qedlogo.png"
    },

    "content_scripts": [
        {
            "matches": [
                "https://chat.qed-verein.de/index.html",
                "https://chat.qed-verein.de/index.html?*",
                "https://chat.qed-verein.de/?*"
            ],
            "js": ["main.js"],
            "run_at": "document_end"
        }
    ],

    "web_accessible_resources": [
        "addons/*.js",
        "js/*.js",
        "js/*.js.map"
    ],
    
    "permissions": [
    ]
}